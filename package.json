    "@ember/render-modifiers": "^1.0.2",
    "@glimmer/component": "^1.0.0",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "babel-jest": "^24.9.0",
    "ember-intl": "5.1.1",
    "ember-power-select": "^4.0.0-beta.4",
